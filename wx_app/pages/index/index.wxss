/**index.wxss**/
.wai{
  padding-bottom: 90px;
}
.good-text{
  width: 100%;
  float: left;
}
.carousel{
  width: 100%;
  height: 350rpx;
}
.carousel .carousel-image{
  width: 100%;
  height: 80%;
  border-radius: 9rpx;
}
.carousel .quiet {
  transform: scale(0.8333333);
  transition: all 0.5s ease-in 0s;
}
.carousel .active {
  transform: none;
  transition: all 0.5s ease-in 0s;
}